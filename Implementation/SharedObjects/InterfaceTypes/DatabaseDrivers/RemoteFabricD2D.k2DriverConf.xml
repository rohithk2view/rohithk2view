<?xml version="1.0" encoding="utf-8"?>
<JdbcDriverConf xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <Name>RemoteFabricD2D</Name>
  <ClassName>shaded.com.k2view.fabric.jdbcex.k2driver.FabricDriver</ClassName>
  <UrlTemplate>jdbc:fabric://[HOST]:[PORT]/[DATABASE]</UrlTemplate>
  <DefaultPort>5401</DefaultPort>
  <PoolEnabled>true</PoolEnabled>
  <ValidationQuery>select 1</ValidationQuery>
  <TestOnReturn>false</TestOnReturn>
