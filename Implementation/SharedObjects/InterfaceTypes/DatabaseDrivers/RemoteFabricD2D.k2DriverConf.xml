  <MetadataQueryColumnsList><![CDATA[]]></MetadataQueryColumnsList>
  <MetadataQueryFksList><![CDATA[]]></MetadataQueryFksList>
  <MetadataQueryPksList><![CDATA[]]></MetadataQueryPksList>
  <ConnectionProperties>
    <Property name="LUNAME">
      <Value><![CDATA[]]></Value>
      <EnableInterfaceOverride>false</EnableInterfaceOverride>
      <ConnectionPropertiesMap>true</ConnectionPropertiesMap>
    </Property>
  </ConnectionProperties>
</JdbcDriverConf>